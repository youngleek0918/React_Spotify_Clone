ongs */}\n                {discover_weekly?.tracks.items.map((item) => (\n                    <SongRow track={item.track} />\n                ))}\n            </div>\n\n\n\n        </div>\n    )\n}\n\nexport default Body\n","/Users/kayounglee/Desktop/project/react/spotify-clone/src/Footer.js",[],"/Users/kayounglee/Desktop/project/react/spotify-clone/src/SidebarOption.js",[],"/Users/kayounglee/Desktop/project/react/spotify-clone/src/Header.js",["66"],"/Users/kayounglee/Desktop/project/react/spotify-clone/src/SongRow.js",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":null,"fatal":true,"severity":2,"message":"71","line":44,"column":2},{"ruleId":"72","severity":1,"message":"73","line":10,"column":27,"nodeType":"74","messageId":"75","endLine":10,"endColumn":35},{"ruleId":"72","severity":1,"message":"73","line":11,"column":33,"nodeType":"74","messageId":"75","endLine":11,"endColumn":41},{"ruleId":"72","severity":1,"message":"73","line":8,"column":22,"nodeType":"74","messageId":"75","endLine":8,"endColumn":30},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],"Parsing error: Unexpected token\n\n\u001b[0m \u001b[90m 42 | \u001b[39m        })\u001b[33m;\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 43 | \u001b[39m      })\u001b[33m;\u001b[39m\u001b[0m\n\u001b[0m\u001b[31m\u001b[1m>\u001b[22m\u001b[39m\u001b[90m 44 | \u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<<\u001b[39m\u001b[33m<\u001b[39m \u001b[33mHEAD\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m    | \u001b[39m \u001b[31m\u001b[1m^\u001b[22m\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 45 | \u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 46 | \u001b[39m      \u001b[90m// 37i9dQZEVXcJZyENOWUFo7\u001b[39m\u001b[0m\n\u001b[0m \u001b[90m 47 | \u001b[39m      \u001b[90m// 3kxjroolleb0u3e1jerh30b1e \u001b[39m\u001b[0m","no-unused-vars","'dispatch' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]